<html lang="en"></html>
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Provinsi</title>
    <!-- Bootstrap CSS -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <!-- Font Awesome -->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css"
    />
  </head>
  <body>
    <div class="me-auto ms-2 mt-2"><a href="/"><i class="fa-solid fa-house fs-3"></i></a></div>
    <div class="container mt-5">
      <div class="row justify-content-center">
        <div class="col-md-6 text-center">
          <!-- Profile Image -->
          <img src="https://via.placeholder.com/150" class="profile-img mb-4" alt="Profile Image">

          <!-- Name -->
          <h3 id="provinsiName">Provinsi Name</h3>

          <!-- Birthdate -->
          <p class="text-muted" id="birthdate">Established: January 1, 2000</p>

          <!-- Island -->
          <p id="island">Located on: Sumatra Island</p>
        </div>
      </div>
    </div>
   
    <!-- Bootstrap Script -->
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
    ></script>
  </body>
</html>